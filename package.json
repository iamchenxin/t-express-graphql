{
  "name": "t-express-graphql",
  "version": "0.0.1",
  "description": "Private modified express-graphql v0.5.1.",
  "contributors": [
    "Lee Byron <lee@leebyron.com> (http://leebyron.com/)",
    "Daniel Schafer <dschafer@fb.com>"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/iamchenxin/t-express-graphql/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/iamchenxin/t-express-graphql.git"
  },
  "keywords": [
    "express",
    "graphql",
    "middleware",
    "api"
  ],
  "main": "dist/index.js",
  "directories": {
    "lib": "./dist"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "PATENTS"
  ],
  "options": {
    "mocha": "--require resources/mocha-bootload src/**/__tests__/**/*.js"
  },
  "scripts": {
    "test": "npm run lint && npm run check && npm run testonly",
    "testonly": "mocha $npm_package_options_mocha",
    "lint": "eslint src",
    "check": "flow check",
    "build": "rm -rf dist/* && babel src --ignore __tests__ --out-dir dist"
  },
  "dependencies": {
    "content-type": "~1.0.1",
    "http-errors": "~1.3.1",
    "raw-body": "~2.1.2"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "babel-core": "5.8.22",
    "babel-eslint": "4.1.8",
    "babel-runtime": "5.8.20",
    "body-parser": "^1.14.0",
    "eslint": "1.1.0",
    "eslint-plugin-babel": "2.1.1",
    "express": "4.13.3",
    "express3": "*",
    "flow-bin": "0.21.0",
    "flow-graphql": "^0.6.2",
    "isparta": "3.0.3",
    "multer": "1.0.3",
    "sane": "1.1.3"
  },
  "peerDependencies": {
    "flow-graphql": "^0.6.2"
  }
}
